{
  "name": "extension-skeleton",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "watch:cabinet": "ng build --project=cabinet --watch --configuration=development",
    "start:cabinet": "ng serve --project=cabinet --configuration=browser --port=4200",
    "build:cabinet": "ng build --project=cabinet --configuration=production",
    "watch:popup": "ng build --project=popup --watch --configuration=development",
    "start:popup": "ng serve --project=popup --configuration=browser --port=4201",
    "build:popup": "ng build --project=popup --configuration=production",
    "start:ts": "webpack --mode development --config ./projects/background/webpack.config.js --env=browser -w",
    "watch:ts": "webpack --mode development --config ./projects/background/webpack.config.js --env=development -w",
    "build:ts": "webpack --config ./projects/background/webpack.config.js --env=production --devtool=false",
    "shared:storybook": "start-storybook -p 6006",
    "build": "npm run copy && npm run build:ts && npm run build:cabinet && npm run build:popup",
    "copy": "copyfiles -f ./projects/extension/* ./dist",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ngneat/until-destroy": "9.2.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.0",
    "@storybook/addon-essentials": "7.0.0-beta.17",
    "@storybook/addons": "7.0.0-beta.17",
    "@storybook/angular": "7.0.0-beta.17",
    "@storybook/builder-webpack5": "7.0.0-beta.17",
    "@storybook/manager-webpack5": "7.0.0-alpha.10",
    "@types/jasmine": "~4.3.0",
    "copyfiles": "^2.4.1",
    "dotenv-webpack": "^8.0.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ts-loader": "9.4.2",
    "ts-node": "9.1.1",
    "tsc-watch": "5.0.3",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "~4.8.2",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  }
}
